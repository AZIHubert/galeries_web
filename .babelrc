{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        ["module-resolver", {
            "root": ["."],
            "alias": {
              "#components": "./src/components",
              "#containers": "./src/containers",
              "#helpers": "./src/helpers",
              "#ressources": "./src/ressources"
            }
         }]
    ]
}